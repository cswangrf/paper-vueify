{
  "name": "paper-vueify",
  "version": "1.1.0",
  "author": "luz-alphacode <luz_alphacode@outlook.com>",
  "description": "A vue adaptation of paper.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name paper-vueify ./src/lib.ts",
    "lint": "vue-cli-service lint",
    "demo": "vue-cli-service build --dest docs --mode development",
    "docs:dev": "vuepress dev dev-docs",
    "docs:build": "vuepress build dev-docs"
  },
  "main": "./dist/paper-vueify.umd.js",
  "typings": "dist/types/lib.d.ts",
  "keywords": [
    "paper",
    "vue",
    "canvas"
  ],
  "dependencies": {
    "arrayex": "^1.0.6",
    "core-js": "^2.6.5",
    "direct-object": "^1.0.7",
    "paper": "^0.12.0",
    "paper-vueify-datatypes": "^1.0.0",
    "vue": "^2.6.6",
    "vue-highlightjs": "^1.3.3"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.3",
    "@types/paper": "^0.11.10",
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-typescript": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "highlight.js": "^9.15.6",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "typescript": "^3.2.1",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^8.0.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.11"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luz-alphacode/paper-vueify.git"
  },
  "bugs": {
    "url": "https://github.com/luz-alphacode/paper-vueify/issues"
  },
  "homepage": "https://github.com/luz-alphacode/paper-vueify#readme",
  "license": "MIT"
}
